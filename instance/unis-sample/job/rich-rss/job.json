{
	"browse": {
		"events_list": "{.base_url}/events"
	},
			
	"extract": {
		"list": {
			"item": "//div[@class='events-list']/ul/li/div",
			"fields": {
				"title": "/h3[0]",
				"datestart": {
					"path": "/div[0]",
					"property": "text",
					"filter": {
						"regex": "(\d{4}-\d{2}-\d{2})\s*-\s*\d{4}-\d{2}-\d{2}"
					}
				},
				"dateend": {
					"path": "/div[0]",
					"property": "text",
					"filter": {
						"regex": "\d{4}-\d{2}-\d{2}\s*-\s*(\d{4}-\d{2}-\d{2})"
					}
				},
				"summary": "/p[0]",
				"venue": "/div[1]",
				"rating": {
					"path": "/div[1]",
					"property": "html",
					"filter": {
						"callback": {
							"name": "rating",
							"args": "[[0,1,3,5,7,10]]"
						}
					}
				}
			},
			"details_url": "/h3[0]/a[0]"
		},
		"details": {
			"fields": {
				"title": "//h1",
				"dates": "//div[@class='when']/span[0]",
				"description": "//div[@class='description']",
				"image": "//div[@class='image']/a/img",
				"venue": "//div[@class='where']/p",
				"email": "//div[@class='contact']/span[0]",
				"website": "//div[@class='contact']/span[1]"
			}
		}
	}
}
